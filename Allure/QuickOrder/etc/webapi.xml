<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/quickorder/:order_increment_id" method="GET">
        <service class="Allure\QuickOrder\Api\QuickOrderInterface" method="getOrder"/>
        <resources>
            <resource ref="Allure_QuickOrder::quickorder"/>
        </resources>
    </route>
</routes>